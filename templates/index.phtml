<br/>
<div id="legend">
    Left: Begin <br/>
    Right: Start
</div>
<br/>
<button class="steps">Input</button>
</body>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<script>
    var currentStep = 1;

    $('.steps').click(function () {
        var processStep = new Promise(function (resolve, reject) {
            if (currentStep == 1) {
                $('.step0').removeClass('current');
                $('.step1').addClass('current');
                $('#legend').html('Right: Generate value for A<br/>Down: Accept value for A');
            } else if (currentStep == 2) {
                $('.step1').removeClass('current');
                $('.step2').addClass('current');
                $('#legend').html('Left: Accept value for B<br/>Down: Calculate Total');
            } else if (currentStep == 3) {
                $('.step2').removeClass('current');
                $('.step3').addClass('current');
                $('#legend').html('Left: Total == A / B<br/>Down: Total = A / B');
            } else if (currentStep == 4) {
                $('.step3').removeClass('current');
                $('.step4').addClass('current');
                $('#legend').html('Left: Print Total<br/>Up: Total == Total');
            } else if (currentStep == 5) {
                $('.step4').removeClass('current');
                $('.step5').addClass('current');
                $('#legend').html('Right: Terminate<br/>Up: End');
            } else if (currentStep == 6) {
                $('.step5').removeClass('current');
                $('.step6').addClass('current');
                $('#legend').html("Congrats! You've completed the maze!");
            }
            resolve();
        });

        // next step
        processStep.then(function(){currentStep += 1});
    });
</script>
</html>
